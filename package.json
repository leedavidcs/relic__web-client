{
	"name": "web-client",
	"version": "0.1.0",
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/leedavidcs/web-client.git"
	},
	"homepage": "https://github.com/leedavidcs/web-client.git#readme",
	"bugs": {
		"url": "https://github.com/leedavidcs/web-client.git/issues"
	},
	"scripts": {
		"audit-ci": "audit-ci --config=./config/audit-ci.config.json",
		"build:dev": "webpack --config scripts/dev.config.js --watch",
		"build:prod": "node scripts/build.js",
		"create-readme": "ts-node --files ./scripts/generate-readme",
		"docker": "npm run docker:build && npm run docker:run",
		"docker:build": "docker build . -t react-docker",
		"docker:run": "docker container run --rm -p 80:8080 react-docker",
		"format:fix": "npm run format -- --write",
		"format": "prettier ./**/*.{json,js,jsx,ts,tsx} -l",
		"lint": "tslint -p .",
		"reset-package": "ts-node --files ./scripts/reset-package-json",
		"server": "ts-node --files ./index",
		"server:watch": "nodemon --watch './server/**/*' -e ts,tsx --watch './server.ts' --exec 'ts-node --files ./index'",
		"start": "npm run build:prod && npm run server",
		"test": "node scripts/test.js",
		"test:coverage": "npm run test -- --coverage",
		"test:sonar": "ts-node scripts/generate-sonar && npm run test -- --coverage --testResultsProcessor=jest-sonar-reporter"
	},
	"devDependencies": {
		"@types/commander": "^2.12.2",
		"@types/enzyme": "^3.9.1",
		"@types/enzyme-adapter-react-16": "^1.0.5",
		"@types/jest": "^24.0.11",
		"@types/nunjucks": "^3.1.1",
		"@types/read-pkg": "^3.0.0",
		"@types/supertest": "^2.0.7",
		"audit-ci": "^1.5.0",
		"babel-jest": "^24.7.1",
		"commander": "^2.20.0",
		"enzyme": "^3.9.0",
		"enzyme-adapter-react-16": "^1.12.1",
		"gitdown": "^2.5.7",
		"husky": "^1.3.1",
		"jest": "^24.7.1",
		"jest-pnp-resolver": "1.0.2",
		"jest-resolve": "23.6.0",
		"jest-sonar-reporter": "^2.0.0",
		"jest-watch-typeahead": "^0.2.1",
		"nodemon": "^1.18.11",
		"nunjucks": "^3.2.0",
		"prettier": "^1.16.4",
		"read-pkg": "^4.0.1",
		"simple-git": "^1.110.0",
		"supertest": "^3.4.2",
		"ts-jest": "^24.0.2",
		"tslint": "^5.15.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-react": "^3.6.0",
		"webpack-cli": "^3.3.0"
	},
	"dependencies": {
		"@babel/core": "7.2.2",
		"@svgr/webpack": "4.1.0",
		"@types/cors": "^2.8.4",
		"@types/express": "^4.16.1",
		"@types/fs-extra": "^5.0.5",
		"@types/helmet": "0.0.42",
		"@types/history": "^4.7.2",
		"@types/lodash": "^4.14.121",
		"@types/morgan": "^1.7.35",
		"@types/node": "^11.13.2",
		"@types/react": "^16.8.3",
		"@types/react-dom": "^16.8.2",
		"@types/react-jss": "^8.6.2",
		"@types/react-redux": "^7.0.1",
		"@types/react-router-dom": "^4.3.1",
		"@types/redux": "^3.6.0",
		"@types/redux-logger": "^3.0.7",
		"@types/winston": "^2.4.4",
		"autobind-decorator": "^2.4.0",
		"babel-core": "7.0.0-bridge.0",
		"babel-loader": "8.0.5",
		"babel-plugin-named-asset-import": "^0.3.1",
		"babel-preset-react-app": "^7.0.1",
		"bfj": "6.1.1",
		"case-sensitive-paths-webpack-plugin": "2.2.0",
		"connected-react-router": "^6.3.1",
		"cors": "^2.8.5",
		"css-loader": "1.0.0",
		"dotenv": "6.0.0",
		"dotenv-expand": "4.2.0",
		"express": "^4.16.4",
		"file-loader": "2.0.0",
		"fork-ts-checker-webpack-plugin-alt": "0.4.14",
		"fs-extra": "^7.0.1",
		"helmet": "^3.15.1",
		"history": "^4.7.2",
		"html-webpack-plugin": "4.0.0-alpha.2",
		"identity-obj-proxy": "3.0.0",
		"jss": "^9.8.7",
		"jss-preset-default": "^4.5.0",
		"lodash": "^4.17.11",
		"mini-css-extract-plugin": "0.5.0",
		"morgan": "^1.9.1",
		"optimize-css-assets-webpack-plugin": "5.0.1",
		"pnp-webpack-plugin": "1.2.1",
		"postcss-flexbugs-fixes": "4.1.0",
		"postcss-loader": "3.0.0",
		"postcss-preset-env": "6.5.0",
		"postcss-safe-parser": "4.0.1",
		"react": "^16.8.2",
		"react-app-polyfill": "^0.2.1",
		"react-dev-utils": "^7.0.3",
		"react-dom": "^16.8.2",
		"react-jss": "^8.6.1",
		"react-redux": "^6.0.0",
		"react-router-dom": "^4.3.1",
		"redux": "^4.0.1",
		"redux-devtools-extension": "^2.13.8",
		"redux-logger": "^3.0.6",
		"redux-observable": "^1.0.0",
		"resolve": "1.10.0",
		"rxjs": "^6.4.0",
		"sass-loader": "7.1.0",
		"style-loader": "0.23.1",
		"terser-webpack-plugin": "1.2.2",
		"ts-node": "^8.0.2",
		"typesafe-actions": "^3.1.0",
		"typescript": "^3.4.3",
		"url-loader": "1.1.2",
		"webpack": "4.28.3",
		"webpack-manifest-plugin": "2.0.4",
		"winston": "^3.2.1",
		"workbox-webpack-plugin": "3.6.3"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"jest": {
		"coverageDirectory": "./coverage",
		"collectCoverageFrom": [
			"src/**/*.{ts,tsx}",
			"server/**/*.{ts,tsx}",
			"*.{ts,tsx}",
			"!build/**/*",
			"!config/**/*",
			"!coverage/**/*",
			"!logs/**/*",
			"!public/**/*",
			"!scripts/**/*",
			"!test-reports/**/*",
			"!**/*.d.ts"
		],
		"coverageReporters": [
			"cobertura",
			"json",
			"lcov",
			"text"
		],
		"resolver": "jest-pnp-resolver",
		"setupFiles": [
			"react-app-polyfill/jsdom"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/config/setup-tests.ts"
		],
		"testMatch": [
			"<rootDir>/(src|server)/**/*.(spec|test).{ts,tsx}",
			"<rootDir>/*.(spec|test).{ts,tsx}"
		],
		"testEnvironment": "jsdom",
		"testURL": "http://localhost",
		"transform": {
			"^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
			"^.+\\.tsx?$": "ts-jest",
			"^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
			"^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
		},
		"transformIgnorePatterns": [
			"[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
			"^.+\\.module\\.(css|sass|scss)$"
		],
		"moduleNameMapper": {
			"^react-native$": "react-native-web",
			"^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
		},
		"moduleFileExtensions": [
			"web.js",
			"js",
			"web.ts",
			"ts",
			"web.tsx",
			"tsx",
			"json",
			"web.jsx",
			"jsx",
			"node"
		]
	},
	"jestSonar": {
		"reportPath": "test-reports",
		"reportFile": "sonar.xml",
		"indent": 4
	},
	"babel": {
		"presets": [
			"react-app"
		]
	},
	"husky": {
		"hooks": {
			"pre-push": "npm run create-readme -- --commit"
		}
	}
}
